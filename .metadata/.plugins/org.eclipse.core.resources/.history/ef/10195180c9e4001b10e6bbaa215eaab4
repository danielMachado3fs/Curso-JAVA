package classes_metodos.exemplos_classe;

public class DataTeste {
	
	public static void main(String[] args) {
		Data d1 = new Data();
		Data d2 = new Data();
		
		d1.ano = 2021;
		d1.mes = 07;
		d1.dia = 13;
		
		d2.ano = 1998;
		d2.mes = 02;
		d2.dia = 18;
		
		System.out.println("Data 1: %d/%d/%d", d1.dia,d1.mes,d1.ano);
	}

}
