package aula1.exercicios;

public class N10ImprimirFigura {
	
	public static void main(String[] args) {
		String figura = "*";
		
		for(; figura.equals("*********"); figura += "**" ) {
			System.out.println(figura);
		}
		
		String figura2 = "*\n***\n*****\n*******\n*********";
		System.out.println(figura2);
	}
	
	
	

}
