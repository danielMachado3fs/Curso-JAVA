package aula1.exercicios;

import javax.swing.JOptionPane;

public class N12Fibonacci {
	
	public static void main(String[] args) {
		String sn = JOptionPane.showInputDialog("Informe a quantidade de números desejados");
		int n = Integer.parseInt(sn);
		JOptionPane.showMessageDialog(null, fibonacci(n));
			
	}
	
	public static int fibonacci(int n) {
		int anterior = 0, proximo = 1, resultado = 0;
		if(n == 0 || n == 1) {
			return 0;
		}else {
			for(int cont = 1; cont <= (n-2); cont++) {
				resultado = proximo + anterior;
				anterior = proximo;
				proximo = resultado;
			}
		}
		return resultado;		
	}

}
